{
    "name": "danielgp/nis2mysql",
    "description": "This package is intended to normalize variaous setting to user MySQL databases",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":     "Daniel Popiniuc",
            "email":    "danielpopiniuc@gmail.com",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-mysqli": "*",
        "danielgp/common-lib": ">=1.1.8",
        "inetsys/phpgettext":">=1.0.11"
    },
    "autoload": {
        "psr-4": {
            "": "source/",
            "danielgp\\common_lib\\": "vendor/danielgp/common-lib/"
        },
        "classmap": [
            "source/AppQueries.php",
            "source/ChangeMySqlAdvancedFeatures.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable":    true,
    "archive": {
        "exclude": ["/nbproject", "*.po", "/scss", "/js/tabber.js", "/vendor", "/tests"]
    }
}
